{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@": ["./src/*"],
      "@assets/*": ["./src/assets/*"],
      "@components/*": ["./src/components/*"],
      "@config/*": ["./src/config/*"],
      "@routes/*": ["./src/routes/*"],
      "@views/*": ["./src/views/*"],
      "underscore": ["lodash"]
    }
  }
}
